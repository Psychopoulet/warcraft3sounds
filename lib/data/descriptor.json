{
	"openapi": "3.0.2",
	"info": {
		"title": "warcraft3sounds",
		"version": "0.2.1",
		"description": "An api for warcraft 3 sounds",
		"contact": {
			"name": "SÃ©bastien VIDAL",
			"url": "https://github.com/Psychopoulet/warcraft3sounds/issues"
		},
		"license": {
			"name": "ISC",
			"url": "https://en.wikipedia.org/wiki/ISC_license"
		}
	},
	"paths": {
		"/api/descriptor": {
			"get": {
				"description": "",
				"summary": "API Swagger",
				"responses": {
					"200": {
						"description": "Everything is fine",
						"content": {
							"application/json": {
								"schema": {
									"type": "object"
								}
							}
						}
					},
					"default": {
						"description": "An error occured",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					}
				}
			}
		},
		"/api/ips": {
			"get": {
				"description": "",
				"summary": "Get server IPs",
				"operationId": "getIPs",
				"responses": {
					"200": {
						"description": "Everything is fine",
						"content": {
							"application/json": {
								"schema": {
									"type": "array",
									"items": {
										"uniqueItems": true,
										"type": "object",
										"properties": {
											"address": {
												"type": "string"
											},
											"name": {
												"type": "string"
											}
										},
										"required": [ "address", "name" ]
									}
								}
							}
						}
					},
					"default": {
						"description": "An error occured",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					}
				}
			}
		},
		"/public/sounds/{sound}": {
			"get": {
				"description": "",
				"summary": "Get the wanted sound",
				"operationId": "getSound",
				"parameters": [
					{
						"name": "sound",
						"in": "path",
						"description": "Wanted sound",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "Everything is fine",
						"content": {
							"audio/wav": {
								"schema": {
									"type": "string",
									"format": "binary"
								}
							},
							"audio/mpeg": {
								"schema": {
									"type": "string",
									"format": "binary"
								}
							}
						}
					},
					"default": {
						"description": "An error occured",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					}
				}
			}
		},
		"/api/err": {
			"get": {
				"description": "",
				"summary": "Generate an error for tests purpose",
				"responses": {
					"200": {
						"description": "Everything is fine",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					},
					"default": {
						"description": "An error occured",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					}
				}
			}
		}
	},
	"components": {
		"schemas": {
			"Error": {
				"type": "object",
				"required": [
					"code",
					"message"
				],
				"properties": {
					"code": {
						"type": "string"
					},
					"message": {
						"type": "string"
					}
				}
			}
		}
	},
	"externalDocs": {
		"description": "Find out more about this API",
		"url": "https://github.com/Psychopoulet/warcraft3sounds#readme"
	}
}