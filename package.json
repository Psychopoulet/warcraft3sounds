{
  "name": "warcraft3sounds",
  "version": "0.1.3",
  "description": "A basic API for Warcraft 3 sounds",
  "main": "lib/main.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "lint": "npx eslint ./lib/**/*.js ./test/**/*.js",
    "check-updates": "npx check-version-modules",
    "unit-tests": "npx nyc --reporter=html --reporter=text mocha",
    "tests": "npm run-script lint && npm run-script check-updates && npm run-script unit-tests",
    "ci": "npm run-script tests && npx nyc report --reporter=text-lcov | coveralls",
    "start": "node lib/main.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run-script lint",
      "pre-push": "npm run-script tests"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Psychopoulet/warcraft3sounds"
  },
  "keywords": [
    "warcraft",
    "sounds",
    "reign of chaos",
    "frozen throne"
  ],
  "author": "SÃ©bastien VIDAL",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Psychopoulet/warcraft3sounds/issues"
  },
  "dependencies": {
    "checksum": "0.1.1",
    "compression": "1.7.3",
    "cors": "2.8.5",
    "express": "4.16.4",
    "express-fileupload": "1.0.0",
    "helmet": "3.15.0",
    "simplessl": "2.5.2",
    "node-confmanager": "0.9.2",
    "sqlite3": "4.0.4"
  },
  "devDependencies": {
    "check-version-modules": "1.1.0",
    "coveralls": "3.0.2",
    "eslint": "5.12.0",
    "mocha": "5.2.0",
    "nyc": "13.1.0",
    "husky": "1.3.1"
  },
  "homepage": "https://github.com/Psychopoulet/warcraft3sounds#readme",
  "engines": {
    "node": ">=6.0.0"
  }
}
