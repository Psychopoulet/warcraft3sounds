  "description": "A basic API for Warcraft 3 sounds",
  "main": "lib/main.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./gulpfile.js ./lib/**/*.js ./tests/**/*.js",
    "tests": "npm run-script lint && npx gulp mocha",
